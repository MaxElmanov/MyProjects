<?xml version="1.0" encoding="UTF-8" ?>
<Configuration status="info">
    <Appenders>
        <Console name="console">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} %t %-5level %m%n"></PatternLayout>
        </Console>
    </Appenders>

    <Loggers>
        <Logger level="info" name="main.array">
            <AppenderRef ref="console"/>
        </Logger>

        <Root level="error">
            <AppenderRef ref="console"></AppenderRef>
        </Root>
    </Loggers>
</Configuration>